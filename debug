--- # Playbook demo of debug and register statement
- hosts: centos
  remote_user: ansible
  become: yes
  become_method: sudo
  connection: ssh
  gather_facts: no
  tasks: 
  - name: Installing telnet packages
    yum: 
      name: Installing telnet packages
      yum: 
        name: telnet
        state: latest
      register: result
    - debug: var=result 

=============================================
--- 
- name: Debug Playbook
  hosts: web
  become: true
  gather_facts: false
  tasks:
    - name: Reading the file
      command: "cat /etc/resolv.conf"
      register: result
      
    - debug:
        var: result
 now press command== ansible-playbook -i ../hosts debug.yml
